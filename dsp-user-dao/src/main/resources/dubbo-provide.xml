<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	http://code.alibabatech.com/schema/dubbo          
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">


	<dubbo:application name="${dubbo.application}" />
	<dubbo:registry address="${dubbo.registry}" />
	<dubbo:protocol name="dubbo" port="${dubbo.protocol}" />
	
	<dubbo:service interface="com.dsp.service.user.UserService" ref="userService" />
	<dubbo:service interface="com.dsp.service.user.RoleService" ref="roleService" />
	<dubbo:service interface="com.dsp.service.user.PermissionService" ref="permissionService" />

</beans>